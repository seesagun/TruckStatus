<?xml version="1.0" encoding="utf-8"?>

<layout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:orientation="vertical">
    <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
    <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="8dp" tools:ignore="MissingConstraints">
    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:orientation="horizontal"
        android:padding="4dp"
        android:weightSum="15">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_weight="4">
        <TextView
            android:id="@+id/item_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="01/01/2562"
            android:textSize="15dp"
            android:textColor="#000000"
            android:fontFamily="@font/thsarabunnew" />
        <TextView
            android:id="@+id/item_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:fontFamily="@font/thsarabunnew"
            android:textColor="#000000"
            android:textSize="15dp"
            android:text="8.50"
            android:layout_marginLeft="5dp"/>
        </LinearLayout>
        <TextView
            android:id="@+id/item_numberTkk"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ASDFJKL999"
            android:textSize="15dp"
            android:fontFamily="@font/thsarabunnew"
            android:textColor="#000000"

            android:layout_weight="5"/>
        <TextView
            android:id="@+id/item_license"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="9989"
            android:fontFamily="@font/thsarabunnew"
            android:textColor="#000000"
            android:textSize="15dp"
            android:layout_weight="5"/>
        <TextView
            android:id="@+id/item_carNumber"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3"
            android:textSize="15dp"
            android:fontFamily="@font/thsarabunnew"
            android:textColor="#000000"
            android:layout_weight="2"/>
    </LinearLayout>
    </LinearLayout>
    </android.support.constraint.ConstraintLayout>
</layout>